# Application
NODE_ENV=development
APP_PORT=3000
API_PORT=4000

# Database
DATABASE_URL="postgresql://postgres:postgres@localhost:5432/inovasaude?schema=public"
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=inovasaude

# JWT
JWT_SECRET=your-secret-key-change-in-production-must-be-at-least-32-chars-long
JWT_EXPIRES_IN=7d

# Upload
MAX_FILE_SIZE=10485760
UPLOAD_DIR=./uploads

# CORS
CORS_ORIGIN=http://localhost:3000

# Logging
LOG_LEVEL=info